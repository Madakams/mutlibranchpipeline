    agent any

    stages {   
        stage('Master branch') { 
            steps { 
               sh 'echo "This is master branch"' 
            }
        }
     
        stage('sprint1') { 
            steps { 
               sh 'echo "sprint1 has been started from application sie..."'
            }
        }

        stage("Sprint2") { 
             steps { 
                sh 'echo "Sprint will start in next week application..."'
            }
        }  
    }
}
